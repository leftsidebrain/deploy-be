{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "vercel-node@2.0.0"
    }
  },

  "routes": [
    {
      "src": "/(.*)",
      "dest": "/routes/index.js"
    }
  ],

  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/dist" }]
}
