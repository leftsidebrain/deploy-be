{
  "version": 2,
  "functions": {
    "routes/**/*.ts": {
      "runtime": "vercel-node@2.0.0"
    }
  },
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/routes/$1.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/dist"
    }
  ]
}
