{
  "version": 2,
  "functions": {
    "routes/**/*.js": {
      "runtime": "vercel-node@2.0.0"
    }
  },
  "routes": [{ "src": "/(.*)", "dest": "/index.ts" }],

  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/dist" }]
}
