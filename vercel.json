{
  "version": 2,
  "functions": {
    "routes/**/*.js": {
      "runtime": "vercel-node@2.0.0"
    }
  },

  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],

  "rewrites": [
    {
      "source": "/users/(.*)",
      "destination": "/routes/users/[id].js"
    },
    {
      "source": "/(.*)",
      "destination": "/dist"
    }
  ]
}
